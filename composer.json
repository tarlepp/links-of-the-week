{
    "name": "tarlepp/links-of-the-week",
    "description": "Links of the week",
    "type": "project",
    "license": "MIT",
    "keywords": [
        "Symfony",
        "Flex",
        "API"
    ],
    "homepage": "https://github.com/tarlepp/links-of-the-week",
    "authors": [
        {
            "name": "Tarmo Lepp√§nen",
            "email": "tarmo.leppanen@protacon.com",
            "homepage": "https://github.com/tarlepp",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/tarlepp/links-of-the-week/issues"
    },
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "czproject/git-php": "3.14.0",
        "guzzlehttp/guzzle": "6.3.3",
        "symfony/console": "*",
        "symfony/flex": "1.1.1",
        "symfony/framework-bundle": "*",
        "symfony/yaml": "*"
    },
    "require-dev": {
        "easycorp/easy-log-handler": "1.0.7",
        "roave/security-advisories": "dev-master",
        "symfony/debug-bundle": "*",
        "symfony/dotenv": "*",
        "symfony/maker-bundle": "1.8.0",
        "symfony/monolog-bundle": "3.3.0",
        "symfony/var-dumper": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "projectTitle": "Links of the week",
        "repositoryUrl": "https://github.com/tarlepp/links-of-the-week.git",
        "symfony": {
            "allow-contrib": true,
            "require": "4.1.6"
        }
    }
}
